    <rules>
        <fullName>Do Not Contact is CHECKED%3A Asst%2E Email</fullName>
        <actions>
            <name>Blank_Out_Asst_Email_field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 2 OR 4) AND 3</booleanFilter>
        <criteriaItems>
            <field>Account.Do_Not_Contact__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Do_Not_Email__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.RecordTypeId</field>
            <operation>notEqual</operation>
            <value>Business Account</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Credit_Risk__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>When &quot;Do Not Contact&quot; is CHECKED, Asst. Email field is BLANKED OUT.</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
