        <name>Update resolved date to today</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <rules>
        <fullName>Record Opened Date</fullName>
        <actions>
            <name>Update_Created_Date_to_Today</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Complaint__c.Date_Opened__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>When a complaint is created, log the opened date</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Traffic Perfect Order Complaint</fullName>
        <actions>
            <name>Email_Traffic_on_Internal_Complaints</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Complaint__c.Complaint_Type__c</field>
            <operation>equals</operation>
            <value>Perfect Order Process Issue</value>
        </criteriaItems>
        <criteriaItems>
            <field>Complaint__c.Complaint_Department__c</field>
            <operation>equals</operation>
            <value>Traffic,Fulfillment</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Update Closed Date</fullName>
        <actions>
            <name>Update_resolved_date_to_today</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Complaint__c.Complaint_Status__c</field>
            <operation>equals</operation>
            <value>Resolved</value>
        </criteriaItems>
        <description>Updates the date closed field when a complaint is completed</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Update Is Internal Checkbox</fullName>
        <actions>
            <name>Check_is_internal</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Complaint__c.Complaint_Type__c</field>
            <operation>equals</operation>
            <value>Perfect Order Process Issue,Marketing Team Request</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
