<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Importance_Range</fullName>
    <active>true</active>
    <description>1-10</description>
    <errorConditionFormula>NOT(OR (ISNULL(Importance__c),(AND(Importance__c&lt;11, Importance__c&gt;0))))</errorConditionFormula>
    <errorMessage>Importance level must be a number between 1-10</errorMessage>
</ValidationRule>
